    "react": "^16.2.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.4.3",
