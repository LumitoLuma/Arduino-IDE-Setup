    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codecov": "^2.2.0",
    "codemirror": "^5.18.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-fbjs": "^2.0.1",
